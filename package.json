{
  "name": "react-static-example-typescript",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "react-static build",
    "bundle": "react-static bundle",
    "export": "react-static export",
    "serve": "serve dist -p 3000 -s",
    "predeploy": "npm run build",
    "deploy": "netlify deploy --dir dist --message \"$(git show -s --format='%h %s')\"",
    "deploy:prod": "npm run deploy -- --prod",
    "lint": "eslint src static.config.js types --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint src static.config.js types --ext .js,.ts,.jsx,.tsx --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "netlify-cli": "^2.59.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-static": "^7.2.0",
    "react-static-plugin-reach-router": "^7.2.0",
    "react-static-plugin-sass": "^7.3.0",
    "react-static-plugin-sitemap": "^7.2.0",
    "react-static-plugin-source-filesystem": "^7.2.0",
    "react-static-plugin-typescript": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "@types/reach__router": "^1.2",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-react-tools": "^1.1.7",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "react-hot-loader": "^4.12.11",
    "serve": "^11.1.0",
    "stylelint": "^13.3.0",
    "stylelint-config-recommended": "^3.0.0",
    "typescript": "^3.5.3"
  }
}
